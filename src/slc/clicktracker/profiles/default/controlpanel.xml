<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="slc.quickchangenote"
    purge="False">
        
    <configlet
        title="ClickTracker Settings"
        action_id="clicktracker"
        appId="slc.clicktracker"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@clicktracker-settings"
        icon_expr="string:${portal_url}/maintenance_icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>
</object>
